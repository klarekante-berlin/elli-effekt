      "no-unused-vars": "warn", 
      "eqeqeq": "error" 
    }
  },
  "prettier": {
    "singleQuote": true, 
    "trailingComma": "es5", 
    "semi": false 
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
